<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="463.0" prefWidth="493.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ChatController">
   <children>
       <TextArea fx:id="chatArea" editable="false" layoutX="12.0" layoutY="14.0" prefHeight="367.0" prefWidth="464.0" promptText="The dialog" />
       <TextField fx:id="message" layoutX="12.0" onKeyReleased="#inputMessage" layoutY="390.0" prefHeight="60.0" prefWidth="384.0" promptText="Write something..." />
       <Button layoutX="400.0" onAction="#sendMessage" layoutY="390.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="79.0" text="Send" />
   </children>
</AnchorPane>
